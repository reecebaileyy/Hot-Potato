generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model GameState {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  current  String
  previous String
}

model leaderboard {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  address String @unique
  fails   Int
  passes  Int
  wins    Int
}

model tokenImage {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  tokenId     Int      @unique
  imageString String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
